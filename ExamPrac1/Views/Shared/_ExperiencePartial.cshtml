@model ExamPrac1.Models.ExperienceViewModel

<div class="expitem">
    <div class="form-group">
        <label asp-for="Tittle" class="control-label"></label>
        <input asp-for="Tittle" class="form-control" name="Experiences[#].Tittle" />
        <span asp-validation-for="Tittle" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Duration" class="control-label"></label>
        <input asp-for="Duration" class="form-control" name="Experiences[#].Duration" />
        <span asp-validation-for="Duration" class="text-danger"></span>
    </div>
    <button type="button" class="btn btn-danger expbtn">Remove</button>
</div>

